extends layout.pug

block content
	.entry
		.entry-description
			h4 #{entry.name}
			p #{entry.description}
				- var date = new Date(entry.date)
				span.date= `${date.getMonth() + 1}/${date.getFullYear()}`
		.entry-photos
			each photo in entry.Photos
				- var path = "../uploads/" + photo.filename;
				img(src=path style="max-width: 90vw")


